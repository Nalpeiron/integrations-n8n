{
	"name": "@nalpeiron/n8n-nodes-nalpeiron",
	"version": "1.0.2",
	"description": "n8n community node for Nalpeiron Growth Platform",
	"keywords": [
		"n8n-community-node-package",
		"nalpeiron",
		"zentitle2",
		"license-management",
		"software-licensing"
	],
	"license": "MIT",
	"homepage": "https://nalpeiron.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/Nalpeiron/integrations-n8n.git"
	},
	"bugs": {
		"url": "https://github.com/Nalpeiron/integrations-n8n/issues"
	},
	"author": {
		"name": "Nalpeiron",
		"email": "admin@nalpeiron.com"
	},
	"engines": {
		"node": ">=20.15"
	},
	"scripts": {
		"build": "npx rimraf dist && tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lint:fix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
		"generate:all": "npm run generate:zentitle && npm run generate:webhooks && npm run lint:fix && npm run format",
		"generate:zentitle": "npx ts-node generators/openapi-generator.ts --get-only --exclude-zengain",
		"generate:webhooks": "npx ts-node generators/webhook-generator.ts"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/NalpeironApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Nalpeiron/Zentitle2/NalpeironZentitle2.node.js",
			"dist/nodes/Nalpeiron/Zentitle2/NalpeironZentitle2Trigger.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^22.15.30",
		"@typescript-eslint/parser": "~8.32.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"prettier": "^3.5.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
